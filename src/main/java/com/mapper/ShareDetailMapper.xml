<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mapper.ShareDetailMapper">

    <select id="getSharSurlBySid" resultType="com.bean.ShareDetail">
        select * from details where id in(select  sd.did  from share_detail  sd where sd.sid=#{sid})
    </select>
</mapper>
    
    
    
    